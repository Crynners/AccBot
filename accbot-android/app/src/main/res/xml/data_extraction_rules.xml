<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="accbot_encrypted_prefs.xml"/>
        <exclude domain="database" path="accbot_dca.db"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="accbot_encrypted_prefs.xml"/>
        <exclude domain="database" path="accbot_dca.db"/>
    </device-transfer>
</data-extraction-rules>
